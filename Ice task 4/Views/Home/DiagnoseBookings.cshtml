@{
    ViewData["Title"] = "Diagnose Bookings";
}

<div class="container">
    <h1>Booking System Diagnostics</h1>

    <div class="card shadow-sm border-0">
        <div class="card-header bg-primary text-white">
            <h4 class="mb-0">System Information</h4>
        </div>
        <div class="card-body">
            <pre class="bg-light p-3 rounded">@ViewBag.DiagnosticInfo</pre>
        </div>
    </div>
</div>